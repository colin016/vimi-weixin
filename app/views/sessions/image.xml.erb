	<FromUserName><![CDATA[<%= @send_message[:fromUser] %>]]></FromUserName>
	<CreateTime><%= Time.now.to_i %></CreateTime>
	<MsgType><![CDATA[<%= @send_message[:type] %>]]></MsgType>
	<ArticleCount>1</ArticleCount>
	<Articles>
		<item>
		<Title><![CDATA[<%= @send_message[:title] %>]]></Title> 
			<Description><![CDATA[<%= @send_message[:description] %>]]></Description>
			<PicUrl><![CDATA[<%= @send_message[:picurl] %>]]></PicUrl>
